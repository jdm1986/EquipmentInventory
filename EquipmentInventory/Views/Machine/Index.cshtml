
@{
    ViewBag.Title = "Available Equipment";
}

<h2>Available Equipment</h2>

<table class="table table-striped">
	<thead>
		<tr>
			<th>Name</th>
		</tr>
	</thead>
	<tbody>
		@for (var i = 0; i < Model.Machines.Count; i++)
		{
			<tr>
				<td>@Model.Machines[i].MakeModel</td>
			</tr>
		}
	</tbody>
</table>

Total Machines: @Model.TotalMachines
